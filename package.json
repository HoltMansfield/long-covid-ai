{
  "name": "next-drizzle-tailwind-auth",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "run-s start:e2e-db db:create-schema build:e2e _test:e2e:with-app stop:e2e-db",
    "db:create-schema": "run-s e2e:generate e2e:migrate",
    "_test:e2e:with-app": "run-p --race e2e:start e2e:test",
    "generate": "dotenv -e .env.local -- drizzle-kit generate",
    "migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "push": "dotenv -e .env.local -- drizzle-kit push",
    "pull": "dotenv -e .env.local -- drizzle-kit pull",
    "check": "dotenv -e .env.local -- drizzle-kit check",
    "up": "dotenv -e .env.local -- drizzle-kit up",
    "studio": "dotenv -e .env.local -- drizzle-kit studio",
    "start:e2e-db": "node scripts/start-e2e-db.cjs",
    "stop:e2e-db": "node scripts/stop-e2e-db.cjs",
    "e2e:generate": "node scripts/drizzle-e2e.cjs generate",
    "e2e:migrate": "node scripts/drizzle-e2e.cjs migrate",
    "build:e2e": "next build",
    "e2e:start": "dotenv -e .env.e2e -- next start --port 3001",
    "e2e:test": "npx wait-on http://localhost:3001 && dotenv -e .env.e2e -- playwright test --project=anonymous --project=logged-in",
    "e2e:test:anonymous": "npx wait-on http://localhost:3001 && dotenv -e .env.e2e -- playwright test --project=anonymous",
    "e2e:test:logged-in": "npx wait-on http://localhost:3001 && dotenv -e .env.e2e -- playwright test --project=logged-in"
  },
  "dependencies": {
    "@highlight-run/next": "^7.9.23",
    "@highlight-run/node": "^3.12.7",
    "@hookform/resolvers": "^5.0.1",
    "@neondatabase/serverless": "^1.0.0",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/themes": "^3.2.1",
    "@react-email/components": "^0.0.41",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "resend": "^4.5.1",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.17.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "mkcert": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "wait-on": "^7.0.1"
  }
}
